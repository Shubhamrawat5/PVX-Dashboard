generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bday {
  id       Int      @id @default(autoincrement())
  name     String
  username String
  date     DateTime @db.Date
  month    String
  place    String
}

model Admin {
  id        Int     @id @default(autoincrement())
  name      String
  username  String  @unique
  password  String
  auth_token String?
}
