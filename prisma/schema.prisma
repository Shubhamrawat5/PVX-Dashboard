generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bday {
  name     String
  username String
  date     Int
  month    Int
  year     Int?
  place    String
  number   String @id
}

model Admin {
  id        Int     @id @default(autoincrement())
  name      String
  username  String  @unique
  password  String
  auth_token String?
}
